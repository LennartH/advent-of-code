{
  "extends": "../tsconfig.base.json",
  "include": [
    "../util/*.ts",
    "src/**/*",
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "rootDirs": ["./src", "../util"],  /* Specify 'virtual' directories acting as single root. */
    "outDir": "./dist",                /* Specify an output folder for all emitted files. */
  }
}
